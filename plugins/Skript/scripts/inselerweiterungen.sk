command /spawnnpcinfrontofme:
	permission: op
	trigger:
		set {_n} to nbt compound from "{NoAI:1b,VillagerData:{profession:""minecraft:mason"",type:""minecraft:swamp""}}"
		set {_loc} to location 0 blocks above player's target block 
		spawn villager at {_loc} with nbt {_n}
		set display name of last spawned villager to "&2Inselerweiterungen"
		set yaw of last spawned villager to 90