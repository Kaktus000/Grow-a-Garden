on right click on villager:
	if display name of event-entity contains "Samen":
		openseedinv(player)
	else if display name of event-entity contains "Verkaufen":
		make player execute command "/sell hand"
		send "&aUm alles zu verkaufen, nutze /sell all" to player
on join:
	if player's inventory does not contain diamond hoe:
		give diamond hoe to player
on player move:
	if player's y coordinate < 50:
		player's name is not "XNeedNoName"
		teleport player to {growagardenisland::%player's uuid%}
command /sell [<text>]:
	trigger:
		if arg-1 is not set:
			send "&cNutze /sell hand oder /sell all" to player
			stop
		if arg-1 is "hand":
			if player's tool is not air:
				set {_total} to 0
				# Sweet Berries
				if 1 of player's tool is sweet berries:
					while 1 of player's tool is sweet berries:
						remove 1 sweet berries from player's tool
						add 17 to {gardencoins::%player's uuid%}
						add 17 to {_total}
				# Carrots
				else if 1 of player's tool is carrot:
					while 1 of player's tool is carrot:
						remove 1 carrot from player's tool
						if lore of player's tool contains "Rainy":
							add 668 to {gardencoins::%player's uuid%}
							add 668 to {_total}
						else if lore of player's tool contains "Thunder":
							add 3340 to {gardencoins::%player's uuid%}
							add 3340 to {_total}
						else:
							add 334 to {gardencoins::%player's uuid%}
							add 334 to {_total}
				# Potatoes
				else if 1 of player's tool is potato:
					while 1 of player's tool is potato:
						remove 1 potato from player's tool
						add 1600 to {gardencoins::%player's uuid%}
						add 1600 to {_total}
				else if 1 of player's tool is poisonous potato:
					while 1 of player's tool is poisonous potato:
						remove 1 poisonous potato from player's tool
						add 800 to {gardencoins::%player's uuid%}
						add 800 to {_total}
				# Beetroots
				else if 1 of player's tool is beetroot:
					while 1 of player's tool is beetroot:
						remove 1 beetroot from player's tool
						add 13000 to {gardencoins::%player's uuid%}
						add 13000 to {_total}
				# Wheat
				else if 1 of player's tool is wheat:
					while 1 of player's tool is wheat:
						remove 1 wheat from player's tool
						add 65000 to {gardencoins::%player's uuid%}
						add 65000 to {_total}
				# Pumpkin
				else if 1 of player's tool is pumpkin:
					while 1 of player's tool is pumpkin:
						remove 1 pumpkin from player's tool
						add 120000 to {gardencoins::%player's uuid%}
						add 120000 to {_total}
				# Melon
				else if 1 of player's tool is melon:
					while 1 of player's tool is melon:
						remove 1 melon from player's tool
						add 300000 to {gardencoins::%player's uuid%}
						add 300000 to {_total}
				# Spruce Log
				else if 1 of player's tool is spruce log:
					while 1 of player's tool is spruce log:
						remove 1 spruce log from player's tool
						add 500000 to {gardencoins::%player's uuid%}
						add 500000 to {_total}
				# Jungle Log
				else if 1 of player's tool is jungle log:
					while 1 of player's tool is jungle log:
						remove 1 jungle log from player's tool
						add 1000000 to {gardencoins::%player's uuid%}
						add 1000000 to {_total}
				if {_total} > 0:
					if player's cytooxien lang is "en":
						send "&aYou received &e%format({_total})% Gardencoins &afor your items!" to player
					else if player's cytooxien lang is "ru":
						send "&aВы получили &e%format({_total})% Gardencoins &aза свои предметы!" to player
					else if player's cytooxien lang is "fr":
						send "&aTu as reçu &e%format({_total})% Gardencoins &apour tes objets !" to player
					else if player's cytooxien lang is "es":
						send "&a¡Has recibido &e%format({_total})% Gardencoins &apor tus objetos!" to player
					else if player's cytooxien lang is "de-AT":
						send "&aDu host &e%format({_total})% Gardencoins &afür deine Sachn kriagt!" to player
					else if player's cytooxien lang is "de-CH":
						send "&aDu häsch &e%format({_total})% Gardencoins &afür dini Sache übercho!" to player
					else if player's cytooxien lang is "nl":
						send "&aJe hebt &e%format({_total})% Gardencoins &aontvangen voor je items!" to player
					else if player's cytooxien lang is "mxn":
						send "&aDu hast so krass &e%format({_total})% Gardencoins &age-sheesh-t für deine Items bekommen! Poggers!" to player
					else:
						send "&aDu hast &e%format({_total})% Gardencoins &afür deine Items erhalten!" to player				
				else:
					send "&cDieses Item kann nicht verkauft werden." to player
			else:
				send "&cDu hast kein Item in der Hand."
		else if arg-1 is "all":
			set {_total} to 0
			# Sweet Berries
			if player's inventory contains sweet berries:
				while player's inventory contains sweet berries:
					remove 1 sweet berries from player's inventory
					add 17 to {gardencoins::%player's uuid%}
					add 17 to {_total}
			# Carrots
			if player's inventory contains carrot:
				while player's inventory contains carrot with lore "&aThunder x10 Wert":
					remove 1 carrot with lore "&aThunder x10 Wert" from player's inventory
					add 664 to {gardencoins::%player's uuid%}
					add 664 to {_total}
				while player's inventory contains carrot with lore "&aRainy x2 Wert":
					remove 1 carrot with lore "&aRainy x2 Wert" from player's inventory
					add 664 to {gardencoins::%player's uuid%}
					add 664 to {_total}
				while player's inventory contains carrot:
					remove 1 carrot from player's inventory
					add 334 to {gardencoins::%player's uuid%}
					add 334 to {_total}
			# Potatoes
			if player's inventory contains potato:
				while player's inventory contains potato:	
					remove 1 potato from player's inventory
					add 1600 to {gardencoins::%player's uuid%}
					add 600 to {_total}
			if player's inventory contains poisonous potato:
				while player's inventory contains poisonous potato:
					remove 1 poisonous potato from player's inventory
					add 800 to {gardencoins::%player's uuid%}
					add 800 to {_total}
			# Beetroots
			if player's inventory contains beetroot:
				while player's inventory contains beetroot:
					remove 1 beetroot from player's inventory
					add 13000 to {gardencoins::%player's uuid%}
					add 13000 to {_total}
			# Wheat
			if player's inventory contains wheat:
				while player's inventory contains wheat:
					remove 1 wheat from player's inventory
					add 65000 to {gardencoins::%player's uuid%}
					add 65000 to {_total}
			# Pumpkin
			if player's inventory contains pumpkin:
				while player's inventory contains pumpkin:
					remove 1 pumpkin from player's inventory
					add 120000 to {gardencoins::%player's uuid%}
					add 120000 to {_total}
			# Melon
			if player's inventory contains melon:
				while player's inventory contains melon:
					remove 1 melon from player's inventory
					add 300000 to {gardencoins::%player's uuid%}
					add 300000 to {_total}
			# Spruce Sapling
			if player's inventory contains spruce log:
				while player's inventory contains spruce log:
					remove 1 spruce log from player's inventory
					add 500000 to {gardencoins::%player's uuid%}
					add 500000 to {_total}
			# Jungle Sapling
			if player's inventory contains jungle log:
				while player's inventory contains jungle log:
					remove 1 jungle log from player's inventory
					add 1000000 to {gardencoins::%player's uuid%}
					add 1000000 to {_total}
			if {_total} > 0:
				if player's cytooxien lang is "en":
					send "&aYou received &e%format({_total})% Gardencoins &afor your items!" to player
				else if player's cytooxien lang is "ru":
					send "&aВы получили &e%format({_total})% Gardencoins &aза свои предметы!" to player
				else if player's cytooxien lang is "fr":
					send "&aTu as reçu &e%format({_total})% Gardencoins &apour tes objets !" to player
				else if player's cytooxien lang is "es":
					send "&a¡Has recibido &e%format({_total})% Gardencoins &apor tus objetos!" to player
				else if player's cytooxien lang is "de-AT":
					send "&aDu host &e%format({_total})% Gardencoins &afür deine Sachn kriagt!" to player
				else if player's cytooxien lang is "de-CH":
					send "&aDu häsch &e%format({_total})% Gardencoins &afür dini Sache übercho!" to player
				else if player's cytooxien lang is "nl":
					send "&aJe hebt &e%format({_total})% Gardencoins &aontvangen voor je items!" to player
				else if player's cytooxien lang is "mxn":
					send "&aDu hast so krass &e%format({_total})% Gardencoins &age-sheesh-t für deine Items bekommen! Poggers!" to player
				else:
					send "&aDu hast &e%format({_total})% Gardencoins &afür deine Items erhalten!" to player		
			else:
				if player's cytooxien lang is "en":
					send "&cYou have no items to sell." to player
				else if player's cytooxien lang is "ru":
					send "&cУ вас нет предметов для продажи." to player
				else if player's cytooxien lang is "fr":
					send "&cTu n'as aucun objet à vendre." to player
				else if player's cytooxien lang is "es":
					send "&cNo tienes items para vender." to player
				else if player's cytooxien lang is "de-AT":
					send "&cDu host nix zum vakaffa." to player
				else if player's cytooxien lang is "de-CH":
					send "&cD'hast kei Sache zum verchaufe." to player
				else if player's cytooxien lang is "nl":
					send "&cJe hebt geen items om te verkopen." to player
				else if player's cytooxien lang is "mxn":
					send "&cBro, du hast einfach keine Items am Start, um die zu verkaufen. Traurig, aber wahr." to player
				else:
					send "&cDu hast keine Items zum Verkaufen." to player
		else:
			if player's cytooxien lang is "en":
				send "&cUse /sell hand or /sell all" to player
			else if player's cytooxien lang is "ru":
				send "&cИспользуйте /sell hand или /sell all" to player
			else if player's cytooxien lang is "fr":
				send "&cUtilise /sell hand ou /sell all" to player
			else if player's cytooxien lang is "es":
				send "&cUsa /sell hand o /sell all" to player
			else if player's cytooxien lang is "de-AT":
				send "&cVawend /sell hand oda /sell all" to player
			else if player's cytooxien lang is "de-CH":
				send "&cNutz /sell hand oder /sell all" to player
			else if player's cytooxien lang is "nl":
				send "&cGebruik /sell hand of /sell all" to player
			else if player's cytooxien lang is "mxn":
				send "&cDigga, mach /sell hand oder /sell all, was ist daran so schwer? Cringe." to player
			else:
				send "&cNutze /sell hand oder /sell all" to player

on tab complete for "/sell":
	set tab completions for position 1 to "hand" and "all"
on join:
	if {sweetberries::%player's uuid%} is not set:
		set {sweetberries::%player's uuid%} to 0
	if {carrots::%player's uuid%} is not set:
		set {carrots::%player's uuid%} to 0
	if {potatoes::%player's uuid%} is not set:
		set {potatoes::%player's uuid%} to 0
	if {beetroots::%player's uuid%} is not set:
		set {beetroots::%player's uuid%} to 0
	if {wheat::%player's uuid%} is not set:	
		set {wheat::%player's uuid%} to 0
	if {pumpkin::%player's uuid%} is not set:
		set {pumpkin::%player's uuid%} to 0
	if {melon::%player's uuid%} is not set:
		set {melon::%player's uuid%} to 0
	if {spruce::%player's uuid%} is not set:
		set {spruce::%player's uuid%} to 0
	if {jungle::%player's uuid%} is not set:
		set {jungle::%player's uuid%} to 0
	if {gardencoins::%player's uuid%} is not set:
		set {gardencoins::%player's uuid%} to 50

#Sell Price = Kaufpreis * 1.3 - 1.5 bei Einmal
function openseedinv(player: player):
	open chest inventory with 6 rows named "&8[&aSamen&8]" to {_player}
	if {sweetberries::%{_player}'s uuid%} > 0:
		set slot 0 of {_player}'s current inventory to {sweetberries::%{_player}'s uuid%} of sweet berries named "&aSweet Berries" with lore "&7Auf Vorrat: %{sweetberries::%{_player}'s uuid%}%%nl%&aPreis: 50 Gardencoins"
	else:
		set slot 0 of {_player}'s current inventory to sweet berries named "&cSweet Berries" with lore "&7Ausverkauft!%nl%&aPreis: 50 Gardencoins"
	if {carrots::%{_player}'s uuid%} > 0:
		set slot 2 of {_player}'s current inventory to {carrots::%{_player}'s uuid%} of carrot named "&aKarotten" with lore "&7Auf Vorrat: %{carrots::%{_player}'s uuid%}%%nl%&aPreis: 500 Gardencoins"
	else:
		set slot 2 of {_player}'s current inventory to carrot named "&cKarotten" with lore "&7Ausverkauft!%nl%&aPreis: 500 Gardencoins"
	if {potatoes::%{_player}'s uuid%} > 0:
		set slot 4 of {_player}'s current inventory to {potatoes::%{_player}'s uuid%} of potato named "&aKartoffeln" with lore "&7Auf Vorrat: %{potatoes::%{_player}'s uuid%}%%nl%&aPreis: 2K Gardencoins"
	else:
		set slot 4 of {_player}'s current inventory to potato named "&cKartoffeln" with lore "&7Ausverkauft!%nl%&aPreis: 2K Gardencoins"
	if {beetroots::%{_player}'s uuid%} > 0:
		set slot 6 of {_player}'s current inventory to {beetroots::%{_player}'s uuid%} of beetroot named "&aRote Beete" with lore "&7Auf Vorrat: %{beetroots::%{_player}'s uuid%}%%nl%&aPreis: 10K Gardencoins"
	else:
		set slot 6 of {_player}'s current inventory to beetroot seeds named "&cRote Beete" with lore "&7Ausverkauft!%nl%&aPreis: 10K Gardencoins"
	if {wheat::%{_player}'s uuid%} > 0:
		set slot 8 of {_player}'s current inventory to {wheat::%{_player}'s uuid%} of wheat named "&aWeizen" with lore "&7Auf Vorrat: %{wheat::%{_player}'s uuid%}%%nl%&aPreis: 50K Gardencoins"
	else:
		set slot 8 of {_player}'s current inventory to wheat seeds named "&cWeizen" with lore "&7Ausverkauft!%nl%&aPreis: 50K Gardencoins"
	if {pumpkin::%{_player}'s uuid%} > 0:
		set slot 10 of {_player}'s current inventory to {pumpkin::%{_player}'s uuid%} of pumpkin named "&aKürbis" with lore "&7Auf Vorrat: %{pumpkin::%{_player}'s uuid%}%%nl%&aPreis: 200K Gardencoins"
	else:
		set slot 10 of {_player}'s current inventory to pumpkin seeds named "&cKürbis" with lore "&7Ausverkauft!%nl%&aPreis: 200K Gardencoins"
	if {melon::%{_player}'s uuid%} > 0:
		set slot 12 of {_player}'s current inventory to {melon::%{_player}'s uuid%} of melon named "&aMelone" with lore "&7Auf Vorrat: %{melon::%{_player}'s uuid%}%%nl%&aPreis: 500K Gardencoins"
	else:
		set slot 12 of {_player}'s current inventory to melon seeds named "&cMelone" with lore "&7Ausverkauft!%nl%&aPreis: 500K Gardencoins"
	if {spruce::%{_player}'s uuid%} > 0:
		set slot 14 of {_player}'s current inventory to {spruce::%{_player}'s uuid%} of spruce sapling named "&aFichte" with lore "&7Auf Vorrat: %{spruce::%{_player}'s uuid%}%%nl%&aPreis: 2M Gardencoins"
	else:
		set slot 14 of {_player}'s current inventory to spruce sapling named "&cFichte" with lore "&7Ausverkauft!%nl%&aPreis: 2M Gardencoins"
	if {_player} has permission "booster":
		if {jungle::%{_player}'s uuid%} > 0:
			set slot 16 of {_player}'s current inventory to {jungle::%{_player}'s uuid%} of jungle sapling named "&aDschungel" with lore "&7Auf Vorrat: %{jungle::%{_player}'s uuid%}%%nl%&aPreis: 5M Gardencoins"
		else:
			set slot 16 of {_player}'s current inventory to jungle sapling named "&cDschungel" with lore "&7Ausverkauft!%nl%&aPreis: 5M Gardencoins"
	else:
		set slot 16 of {_player}'s current inventory to jungle sapling named "&cDschungel" with lore "&dNur für Booster%nl%&aPreis: 5M Gardencoins"
	set slot 18 of {_player}'s current inventory to bone meal named "&aKnochenmehl" with lore "&7Preis: 4K Gardencoins"
